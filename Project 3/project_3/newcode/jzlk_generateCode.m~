function [ codeVal, codeLength ] = jzlk_generateCode( subband )
%Calculates code for coefficients

%[counts, codeVal] = hist(subband(:), 2*max(abs(subband(:))));

serSub = subband(:);
sMin = min(serSub);
sMax = max(serSub);

codeVal = zeros(sMax - sMin);

sum1 = sum(counts);

p = counts./sum1;

codeVal(p==0) = [];
p(p==0) = [];

codeLength = -p.*log2(p);

%entropy = sum(codebook);
%entropy = -sum(p.*log2(p));

end
